              "Colum4_1": " ",
              "Colum1_2": "España",
              "Colum2_2": "Español",
              "Colum3_2": "niño",
              "Colum4_2": " "
            },
            "Q": {
              "name": "Q",
              "Description": "Сложные Буквы",
              "Colum1_1": "Нужна только для того чтобы создать звук К перед е, i ",
              "Colum2_1": "QUE /ке/",
              "Colum3_1": "QUI /ки/",
              "Colum4_1": " ",

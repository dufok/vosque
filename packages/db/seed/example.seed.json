              "Colum4_2": " "

            },
            "K": {
              "name": "K",
              "Description": "Сложные Буквы",
              "Colum1_1": "в заимствованиях из английского языка",
              "Colum2_1": " ",
              "Colum3_1": " ",
              "Colum4_1": " ",
              "Colum1_2": "kiwi",
              "Colum2_2": "kilo",
              "Colum3_2": "kiosko",
              "Colum4_2": " "
            },
            "L": {
              "name": "L",
              "Description": "Сложные Буквы",
              "Colum1_1": "1) перед a, o, u, согл. /Л/",
              "Colum2_1": "2) перед e, i /ЛЬ/ ",
              "Colum3_1": "3) “LL” = /Ж,Ш/ ",
              "Colum4_1": " ",
              "Colum1_2": "lana, lona, luna",
              "Colum2_2": "español, capital",
              "Colum3_2": "me llamo, pollo",
              "Colum4_2": " "
            },
            "Ñ": {
              "name": "Ñ",
              "Description": "Сложные Буквы",
